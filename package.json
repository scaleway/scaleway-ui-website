{
  "name": "scaleway-ui-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint": "eslint --cache --ext tsx,ts,.d.ts .",
    "prepare": "husky install",
    "commit": "npx git-cz -a",
    "svg": "svgo --pretty --multipass",
    "svg:all": "yarn run svg -r -f src"
  },
  "dependenciesMeta": {
    "@react-spring/core": {
      "built": false
    }
  },
  "dependencies": {
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@scaleway/eslint-config-react": "3.3.2",
    "@scaleway/ui": "0.134.5",
    "@sentry/nextjs": "6.16.1",
    "next": "12.0.8",
    "polished": "4.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-schemaorg": "2.0.0",
    "react-syntax-highlighter": "15.4.5",
    "react-use-clipboard": "1.0.7",
    "schema-dts": "1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.0.2",
    "@commitlint/config-conventional": "16.0.0",
    "@emotion/eslint-plugin": "11.7.0",
    "@next/bundle-analyzer": "12.0.8",
    "@types/node": "16.11.19",
    "@types/react": "17.0.38",
    "@types/react-syntax-highlighter": "13.5.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.6.0",
    "eslint-config-next": "12.0.8",
    "husky": "7.0.4",
    "lint-staged": "12.1.7",
    "next-transpile-modules": "9.0.0",
    "prettier": "2.5.1",
    "svgo": "2.8.0",
    "typescript": "4.5.4"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "prettier --write",
      "eslint --fix --ext ts,tsx"
    ],
    "*.svg": [
      "yarn run svg"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "contributors": [
    {
      "name": "Alexandre Philibeaux",
      "email": "aphilibeaux@scaleway.com",
      "url": "https://github.com/philibea"
    },
    {
      "name": "Vincent Audebert",
      "email": "vaudebert@scaleway.com",
      "url": "https://github.com/vincentaudebert"
    },
    {
      "name": "Matthias Prost",
      "email": "mprost@scaleway.com",
      "url": "https://github.com/matthprost"
    },
    {
      "name": "Emmanuel Chambon",
      "email": "echambon@scaleway.com",
      "url": "https://github.com/chambo-e"
    },
    {
      "name": "Dorian Maliszewski",
      "email": "dmaliszewski@scaleway.com",
      "url": "https://github.com/DorianMaliszewski"
    }
  ],
  "packageManager": "yarn@3.1.1"
}
