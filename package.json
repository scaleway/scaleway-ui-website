{
  "name": "scaleway-ui-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint": "eslint --cache --ext tsx,ts,.d.ts .",
    "prepare": "husky install",
    "commit": "npx git-cz -a",
    "svg": "svgo --pretty --multipass",
    "svg:all": "pnpm run svg -r -f src"
  },
  "dependenciesMeta": {
    "@react-spring/core": {
      "built": false
    }
  },
  "dependencies": {
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@scaleway/ui": "0.156.2",
    "@sentry/nextjs": "7.1.1",
    "next": "12.1.6",
    "polished": "4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-schemaorg": "2.0.0",
    "react-syntax-highlighter": "15.5.0",
    "react-use-clipboard": "1.0.8",
    "schema-dts": "1.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.18.5",
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@emotion/eslint-plugin": "11.7.0",
    "@next/bundle-analyzer": "12.1.6",
    "@nivo/core": "0.79.0",
    "@nivo/tooltip": "0.79.0",
    "@scaleway/eslint-config-react": "3.4.33",
    "@types/node": "17.0.25",
    "@types/react": "18.0.12",
    "@types/react-syntax-highlighter": "15.5.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "husky": "8.0.1",
    "lint-staged": "13.0.1",
    "next-transpile-modules": "9.0.0",
    "prettier": "2.6.2",
    "prop-types": "15.8.1",
    "svgo": "2.8.0",
    "typescript": "4.7.3"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "prettier --write",
      "eslint --fix --ext ts,tsx"
    ],
    "*.svg": [
      "pnpm run svg"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "18",
        "react-dom": "18"
      }
    }
  },
  "contributors": [
    {
      "name": "Alexandre Philibeaux",
      "email": "aphilibeaux@scaleway.com",
      "url": "https://github.com/philibea"
    },
    {
      "name": "Vincent Audebert",
      "email": "vaudebert@scaleway.com",
      "url": "https://github.com/vincentaudebert"
    },
    {
      "name": "Matthias Prost",
      "email": "mprost@scaleway.com",
      "url": "https://github.com/matthprost"
    },
    {
      "name": "Emmanuel Chambon",
      "email": "echambon@scaleway.com",
      "url": "https://github.com/chambo-e"
    },
    {
      "name": "Dorian Maliszewski",
      "email": "dmaliszewski@scaleway.com",
      "url": "https://github.com/DorianMaliszewski"
    }
  ],
  "packageManager": "pnpm@7.2.1"
}
